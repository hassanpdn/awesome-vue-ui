{
      "name": "awesome-vue-ui",
      "version": "0.0.4",
      "main": "./dist/vue-ui.umd.js",
      "module": "./dist/vue-ui.es.js",
      "author": "Hassan Pudineh",
      "description": "Awesome UI components for Vue.js 3",
      "files": [
            "dist"
      ],
      "exports": {
            ".": {
                  "import": "./dist/vue-ui.es.js",
                  "require": "./dist/vue-ui.umd.js"
            },
            "./dist/style.css": "./dist/style.css"
      },
      "scripts": {
            "dev": "vite",
            "build": "vite build && npm run test && npm run coverage",
            "preview": "vite preview",
            "test": "vitest --environment jsdom",
            "coverage": "vitest run --coverage"
      },
      "dependencies": {
            "vue": "^3.2.25"
      },
      "devDependencies": {
            "@vitejs/plugin-vue": "^4.0.0",
            "@vue/test-utils": "^2.2.10",
            "autoprefixer": "^10.4.13",
            "jsdom": "^21.1.0",
            "postcss": "^8.4.21",
            "tailwindcss": "^3.2.6",
            "vite": "^3.0.0",
            "vitest": "^0.28.5"
      }
}
