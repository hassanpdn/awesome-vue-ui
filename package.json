{
      "name": "awesome-vue-ui",
     "main": "./dist/vue-ui.umd.js",
      "module": "./dist/vue-ui.es.js",
      "author": "Hassan Pudineh",
      "description": "Awesome UI components for Vue.js 3",
      "publishConfig": {
            "registry": "https://registry.npmjs.org/"
      },
      "repository": "https://github.com/hassanpdn/awesome-vue-ui.git",
      "files": [
            "dist"
      ], 
      "exports": {
            ".": {
                  "import": "./dist/vue-ui.es.js",
                  "require": "./dist/vue-ui.umd.js"
            },
            "./dist/style.css": "./dist/style.css"
      },
      "scripts": {
            "dev": "vite",
            "build": "vite build && npm run test && npm run coverage",
            "preview": "vite preview",
            "test": "vitest --environment jsdom",
            "coverage": "vitest run --coverage"
      },
      "dependencies": { 
            "@semantic-release/git": "^10.0.1",
            "@semantic-release/npm": "^9.0.2",
            "awesome-vue-ui": "^0.1.7",
            "vue": "^3.2.25"
      },
      "devDependencies": { 
            "@semantic-release/changelog": "^6.0.2",
            "@vitejs/plugin-vue": "^4.0.0",
            "@vitest/coverage-c8": "^0.28.5",
            "@vue/test-utils": "^2.2.10",
            "autoprefixer": "^10.4.13",
            "jsdom": "^21.1.0",
            "postcss": "^8.4.21",
            "semantic-release": "^20.1.0",
            "tailwindcss": "^3.2.6",
            "vite": "^4.0.0",
            "vitest": "^0.28.5"
      }
}
